<h1>画像投稿ファーム</h1>
  <%= form_with model: @post_image, url: public_post_images_path do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>動物名</h4>
      <%= f.text_field :name %>
    <h4>説明</h4>
      <%= f.text_area :description %>
    <%= f.submit '投稿' %>
  <% end %>