<h2>プロフィール編集</h2>
<%= form_with model: @user, url: public_user_path(current_user.id) do |f| %>
  <label for="inputName">ニックネーム</label>
  <%= f.text_field :name, autoforcus: true, id:"inputName", placeholder:"ニックネーム" %>
  <label for="inputImage">プロフィール画像</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>